#!/bin/sh

APPDIR=$(readlink -f "$0")
APPDIR=$(dirname "$APPDIR")/linux-aarch64
java -Djna.nosys=true -Dnet.java.games.input.librarypath="$APPDIR/data" -Djava.library.path="$APPDIR:$APPDIR/lib" -cp "$APPDIR:$APPDIR/lib/jinput-2.0.10.jar:$APPDIR/lib/jinput-2.0.10-natives-all.jar:$APPDIR/lib/arcade_snake.jar:$APPDIR/lib/jogl-all-2.5.0-natives-linux-armv6hf.jar:$APPDIR/lib/gluegen-rt-2.5.0-natives-linux-aarch64.jar:$APPDIR/lib/jogl-all-2.5.0-natives-linux-amd64.jar:$APPDIR/lib/core-4.4.4.jar:$APPDIR/lib/gluegen-rt-2.5.0.jar:$APPDIR/lib/jogl-all-2.5.0.jar:$APPDIR/lib/kotlin-stdlib-2.0.20.jar:$APPDIR/lib/gluegen-rt-2.5.0-natives-android-aarch64.jar:$APPDIR/lib/gluegen-rt-2.5.0-natives-linux-armv6hf.jar:$APPDIR/lib/gluegen-rt-main-2.5.0.jar:$APPDIR/lib/annotations-13.0.jar:$APPDIR/lib/jogl-all-2.5.0-natives-macosx-universal.jar:$APPDIR/lib/jogl-all-2.5.0-natives-linux-aarch64.jar:$APPDIR/lib/gluegen-rt-2.5.0-natives-windows-amd64.jar:$APPDIR/lib/gluegen-rt-2.5.0-natives-linux-amd64.jar:$APPDIR/lib/jogl-all-main-2.5.0.jar:$APPDIR/lib/jogl-all-2.5.0-natives-android-aarch64.jar:$APPDIR/lib/gluegen-rt-2.5.0-natives-macosx-universal.jar:$APPDIR/lib/jogl-all-2.5.0-natives-windows-amd64.jar:$APPDIR/lib/antlr-2.7.7.jar:$APPDIR/lib/dashedlines.jar:$APPDIR/lib/sound.jar:$APPDIR/lib/vorbisspi-1.0.3.3.jar:$APPDIR/lib/mp3spi-1.9.5.4.jar:$APPDIR/lib/jportaudio.jar:$APPDIR/lib/tritonus-share-0.3.7.4.jar:$APPDIR/lib/jlayer-1.0.1.4.jar:$APPDIR/lib/jsyn-17.1.0.jar:$APPDIR/lib/jorbis-0.0.17.4.jar" arcade_snake "$@"
